{
  "name": "ttslibrary",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest --coverage",
    "postinstall": "npx patch-package && yarn images && yarn icons",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'src/**'",
    "pod": "cd ios && pod update",
    "images": "node src/theme/assets.js",
    "icons": "node src/components/Icon/script.js",
    "idev": "npx react-native run-ios --simulator='iPhone 11 Pro' --scheme='ttsLibrary-dev'",
    "iqa": "npx react-native run-ios --scheme='ttsLibrary-qa'",
    "iprod": "npx react-native run-ios --scheme='ttsLibrary'",
    "adev": "ENVFILE=.env.dev npx react-native run-android --appId com.android.dev --main-activity MainActivity",
    "aqa": "ENVFILE=.env.qa npx react-native run-android --appId com.android.qa --main-activity MainActivity",
    "aprod": "ENVFILE=.env npx react-native run-android --appId com.app.android --main-activity MainActivity",
    "a-dev-signing": "cd android && ENVFILE=.env.dev ./gradlew :app:signingReport",
    "a-qa-signing": "cd android && ENVFILE=.env.qa ./gradlew :app:signingReport",
    "a-prod-signing": "cd android && ENVFILE=.env ./gradlew :app:signingReport",
    "prestorybook": "rnstl",
    "istorybook": "npx react-native run-ios --simulator='iPhone 11 Pro' --scheme='HelloWorld-storybook'",
    "astorybook": "ENVFILE=.env.storybook npx react-native run-android --appId com.android.dev --main-activity MainActivity",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.6.0",
    "@react-native-async-storage/async-storage": "^1.15.4",
    "@react-native-firebase/app": "^11.2.0",
    "@react-native-firebase/auth": "^11.2.0",
    "@react-native-firebase/crashlytics": "^11.2.0",
    "@react-native-firebase/firestore": "^11.2.0",
    "@react-native-masked-view/masked-view": "^0.2.2",
    "@react-navigation/native": "5.9.2",
    "@react-navigation/stack": "5.14.4",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "i18next": "^20.3.2",
    "proxy-polyfill": "^0.3.2",
    "react": "17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-hook-form": "^7.6.3",
    "react-i18next": "^11.11.0",
    "react-native": "0.64.1",
    "react-native-config": "^1.4.2",
    "react-native-device-info": "7.3.1",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-tts": "^4.1.0",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.2",
    "rn-scaled-sheet": "^1.0.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/faker": "^5.5.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.6",
    "@types/react-native": "^0.63.2",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^16.9.2",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-dom": "16.13.1",
    "react-native-storybook-loader": "https://github.com/mthuong/react-native-storybook-loader.git",
    "react-test-renderer": "17.0.1",
    "reactotron-react-native": "^5.0.0",
    "typescript": "^4.0.2"
  },
  "resolutions": {},
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.ts"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./storybook"
      ],
      "pattern": "**/*.stories.*",
      "outputFile": "storybook/storyLoader.ts"
    }
  }
}
